                        <%if !@calculation.m_e_max.blank?%>
                                <%mn=@calculation.n_e_max*1000.0/(@calculation.n_N*2*Math::PI/60.0)  %>
                                <% x=(@calculation.n_m+0.0)/@calculation.n_N %>
